{
  "name": "slidev-theme-project-documentation",
  "version": "0.0.1",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=14.0.0",
    "slidev": ">=0.19.3"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^0.37.1",
    "codemirror-theme-vars": "^0.1.1",
    "prism-theme-vars": "^0.2.4",
    "sass": "^1.56.1",
    "theme-vitesse": "^0.6.0"
  },
  "devDependencies": {
    "@slidev/cli": "^0.37.1",
    "playwright-chromium": "^1.28.1"
  },
  "//": "Learn more: https://sli.dev/themes/write-a-theme.html",
  "slidev": {
    "colorSchema": "light",
    "highlighter": "all",
    "defaults": {
      "aspectRatio": "297/210",
      "fonts": {
        "sans": "Helvetica Neue, Roboto"
      }
    }
  }
}
